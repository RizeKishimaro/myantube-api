{
  "version": 2,
  "builds": [
    {
      "src": "/var/task/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ],
  "devCommand": "npm run start:dev",
  "buildCommand": "npm i && npm i -g @nestjs/cli && npx prisma migrate reset && npx prisma generate"
}
